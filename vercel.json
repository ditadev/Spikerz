{
    "version": 2,
    "buildCommand": "npm run build -- --configuration production",
    "outputDirectory": "dist/spikerz/browser",
    "framework": null,
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/(.*).js",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/javascript"
          }
        ]
      }
    ]
  }